<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 version="2.0"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd">
    <entity class="cn.mobiledaily.domain.identity.User">
        <named-query name="User.findAll">
            <query>select u from User u</query>
        </named-query>
        <named-query name="User.findByEmail">
            <query>select u from User u where u.email=:email</query>
        </named-query>
        <named-query name="User.findByName">
            <query>select u from User u where username=:name</query>
        </named-query>
    </entity>
    <entity class="cn.mobiledaily.domain.mobile.Attendee">
        <named-query name="Attendee.findAll">
            <query>select a from Attendee a</query>
        </named-query>
        <named-query name="Attendee.findByName">
            <query>select a from Attendee a where dummyUserName=:name</query>
        </named-query>
    </entity>
</entity-mappings>